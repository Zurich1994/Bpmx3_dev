package com.hotent.platform.model.bpm;

import org.apache.commons.lang.builder.ToStringBuilder;


/**
 * 对象功能:抄送消息提醒类型类
 * 开发公司:广州宏天软件有限公司
 * 开发人员:helh
 * 创建时间:2013-8-30 16:44:35
 */
public class CcMessageType {
	//站内消息
	private Integer inner;
	//邮件
	private Integer mail;
	//短信
	private Integer sms;
	
	public Integer getInner() {
		return inner;
	}
	public void setInner(Integer inner) {
		this.inner = inner;
	}
	public Integer getMail() {
		return mail;
	}
	public void setMail(Integer mail) {
		this.mail = mail;
	}
	public Integer getSms() {
		return sms;
	}
	public void setSms(Integer sms) {
		this.sms = sms;
	}
	
	/**
	 * @see java.lang.Object#toString()
	 */
	public String toString() 
	{
		return new ToStringBuilder(this)
		.append("inner", this.inner) 
		.append("mail", this.mail) 
		.append("sms", this.sms) 
		.toString();
	}
}
