/* Reset */
body,img,p,h1,h2,h3,h4,h5,h6,ul,ol {margin:0; padding:0; list-style:none; border:none;}
/* End Reset */
li{
list-style-type :none;
}
body {font-size:12px; font-family:Arial,Verdana,Sans-Serif; background: #FFFFFF;}
a {color:white;}
/* Colors Download by http://www.codefans.net*/
.color-yellow {background:#f2bc00;}
.color-red    {background:#dd0000;}
.color-blue   {background:#92caed;}
.color-white  {background:#dfdfdf;}
.color-orange {background:#f66e00;}
.color-green  {background:#8dc100;}
.color-yellow h3,
.color-white h3,
.color-green h3
	{color:#000;}
.color-red h3,
.color-blue h3,
.color-orange h3
	{color:#FFF;}
/* End Colors */

/* Head section */
#head {
   /* background: #000 url(${ctx}/js/desktop/img/head-bg.png) repeat-x;*/
    height: 100px;
}
#head h1 {
    line-height: 100px;
    color: #FFF;
    text-align: center;
    background: url(${ctx}/js/desktop/img/inettuts.png) no-repeat center;
    text-indent: -9999em
}
/* End Head Section */

/* Columns section */
#columns .column {
    float: left;
    width: 100%;
		/* Min-height: */
		min-height: 400px;
		height: auto !important; 
		height: 400px;
		valign:top;
}

/* Column dividers (background-images) :
	#mycolumns #column1 { background: url(${ctx}/js/desktop/img/head-bg.png) no-repeat right top; }
	#mycolumns #column3 { background: url(${ctx}/js/desktop/img/column-bg-right.png) no-repeat left top; }
 */	
#columns #column1 .widget { margin: 5px 5px 0 5px; }
#columns #column2 .widget { margin: 5px 5px 0 5px; }
#columns #column3 .widget { margin: 5px 5px 0 5px; }
#columns .widget {
    margin: 30px 20px 0 20px;
    padding: 2px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
}
#columns .widget .widget-head {
    color: #000;
    overflow: hidden;
    width: 100%;
    height: 30px;
    line-height: 30px;
}
#columns .widget .widget-head h3 {
    padding: 0 5px;
    float: left;
}
a.contenttw{
	padding: 0 5px;
    color: blue;
    font-size:12px;
    -moz-border-radius-bottomleft: 2px;
    -moz-border-radius-bottomright: 2px;
    -webkit-border-bottom-left-radius: 2px;
    -webkit-border-bottom-right-radius: 2px;
    line-height: 24px;
    overflow: hidden;
    text-decoration: none;
}

a.contenttw:hover{
	text-decoration: underline;
}

#columns .widget .widget-content {
    background: #FFF;
    padding: 0 5px;
    color: #778899;
    -moz-border-radius-bottomleft: 2px;
    -moz-border-radius-bottomright: 2px;
    -webkit-border-bottom-left-radius: 2px;
    -webkit-border-bottom-right-radius: 2px;
    line-height: 1.2em;
    overflow: hidden;
    min-height: 320px;
}
#columns .widget .widget-content p {
    padding: 0.8em 0;
    border-bottom: 1px solid #666;
}
#columns .widget .widget-content img {
    float: right;
    margin: 10px;
    border: 1px solid #FFF;
}
#columns .widget .widget-content pre {
    padding: 0.5em 5px;
    color: #EEE;
    font-size: 12px;
}
#columns .widget .widget-content ul {
    padding: 0px 0 0px 0px;
    list-style: disc;
}
#columns .widget .widget-content ul li {padding: 3px 0;}
#columns .widget .widget-content ul.images {
    padding: 7px 0 0 0;
    list-style: none;
    height: 1%;
}
#columns .widget .widget-content ul.images li {
    display: inline;
    float: left;
}
#columns .widget .widget-content ul.images img {
    display: inline;
    float: left;
    margin: 0 0 7px 7px;
}
/* End Columns section */



.wtMonth {
	border-collapse: collapse;
	table-layout:fixed;
	width:100%;
}
.wtMonth caption {
	text-align: left;
	font-family: normal 120% 宋体, arial;
 	font-size:12px;
	font-weight:normal;
	padding-bottom: 6px;
}

.wtMonth caption .wtDate{
    font-size:150%;
    font-weight:bold;
}

.wtMonth caption .monthFlag{
	font-size: 100%;
	color: red;
}

.wtMonth th {
	border: 1px solid #999;
	border-bottom: none;
	padding: 3px 2px 2px;
	margin:0;
	background-color: #ADD;
	color: #333;
	font: 80% 宋体;
}
.wtMonth td {
	border: 1px solid #AAA;
}
.wtMonth td.nonactivated {
	background-color: #FFFFFF;
	
}
.wtMonth td.active {
	background-color: #FFA500;
}
.wtMonth td.previous, .wtMonth td.next {
	background-color: #eee;
}
.wtMonth td.weekend {
	background-color: #FFCCFF;
}

.Function .FunctionText .Icon1 li {
    background: url("img/index_gif_v2.gif") no-repeat scroll 0 -278px #FFFFFF;
}

.desktop li {
    border-bottom: 1px dotted #888888;
    float: left;
    line-height: 24px;
    padding-left: 15px;
    vertical-align: middle;
    width: 95%;
}



ul, li {
    list-style: none outside none;
}

div, ul, li, span, td, input {
    margin: 0;
    padding: 0;
}

ul, li {
    list-style: none outside none;
}



