<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 客户评级,count(公司名称) as 个数 FROM 客户评级
where 评级日期 like '%2011%'
group by 客户评级]]></Query>
</TableData>
<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p"/>
<O>
<![CDATA[A]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT c.客户评级,round(sum(a.单价*a.数量)/count(c.客户ID),2) as 平均值
FROM S订单明细 a,S订单 b,客户评级 c
where c.客户评级='${p}' and c.评级日期 like '%2011%' and a.订单ID=b.订单ID and b.客户ID=c.客户ID
group by c.客户评级
]]></Query>
</TableData>
<TableData name="ds3" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p"/>
<O>
<![CDATA[A]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT count(客户ID),客户评级 FROM 客户评级
where 客户评级='${p}' and 评级日期 like '%2011%'
group by 客户评级]]></Query>
</TableData>
<TableData name="ds4" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select a.客户评级,个数1 as '2012',个数2 as '2013' from  (SELECT 客户评级, count(客户ID) as 个数2 FROM 客户评级
where 评级日期 like '%2012%'
group by 客户评级) as a
inner join 
(SELECT count(客户ID) as 个数1,客户评级 FROM 客户评级
where 评级日期 like '%2011%'
group by 客户评级) as b
on a.客户评级=b.客户评级

]]></Query>
</TableData>
<TableData name="ds5" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 公司名称,金额,上期金额,金额-上期金额 as 差额 FROM 客户评级
where 评级日期 like '%2011%' and 上期金额 is not null
order by 差额 desc limit 10]]></Query>
</TableData>
<TableData name="ds6" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select aa.客户评级,数量,中间商数量 from (SELECT b.客户评级,count(a.客户ID) as 数量 FROM S客户 a,客户评级 b
where a.客户ID=b.客户ID and b.评级日期 like '%2011%'
group by b.客户评级) as aa
inner join 
(SELECT count(a.客户ID) as 中间商数量,b.客户评级 FROM S客户 a,客户评级 b
where a.客户ID=b.客户ID and b.评级日期 like '%2011%' and a.是否中间商 ='true'
group by b.客户评级) as bb
on aa.客户评级=bb.客户评级]]></Query>
</TableData>
<TableData name="ds7" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select a.客户评级,总额,中间商总额 from (SELECT 客户评级,round(sum(金额),2) as 总额 FROM 客户评级 
where 评级日期 like '%2011%'
group by 客户评级) as a
inner join 
(SELECT 客户评级,round(sum(金额),2) as 中间商总额 FROM 客户评级,S客户 c
where 客户评级.客户ID=c.客户ID and 评级日期 like '%2011%' and 是否中间商 ='true'
group by 客户评级) as b
on a.客户评级=b.客户评级

]]></Query>
</TableData>
