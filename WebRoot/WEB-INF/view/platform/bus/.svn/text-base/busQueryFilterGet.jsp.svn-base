<%@page language="java" pageEncoding="UTF-8"%>
<%@include file="/commons/include/html_doctype.html"%>
<html>
<head>
<title>BUS_QUERY_FILTER明细</title>
<%@include file="/commons/include/get.jsp"%>
<script type="text/javascript">
	//放置脚本
</script>
</head>
<body>
	<div class="panel">
		<div class="panel-top">
			<div class="tbar-title">
				<span class="tbar-label">BUS_QUERY_FILTER详细信息</span>
			</div>
			<div class="panel-toolbar">
				<div class="toolBar">
					<div class="group">
						<a class="link back" href="list.ht"><span></span>返回</a>
					</div>
				</div>
			</div>
		</div>
		<table class="table-detail" cellpadding="0" cellspacing="0" border="0">
			<tr>
				<th width="20%">规则ID:</th>
				<td>${busQueryFilter.ruleid}</td>
			</tr>
			<tr>
				<th width="20%">表名:</th>
				<td>${busQueryFilter.tablename}</td>
			</tr>
			<tr>
				<th width="20%">过滤名称:</th>
				<td>${busQueryFilter.filtername}</td>
			</tr>
			<tr>
				<th width="20%">过滤描述:</th>
				<td>${busQueryFilter.filterdesc}</td>
			</tr>
			<tr>
				<th width="20%">过滤条件Key:</th>
				<td>${busQueryFilter.filterkey}</td>
			</tr>
			<tr>
				<th width="20%">查询参数:</th>
				<td>${busQueryFilter.queryparameter}</td>
			</tr>
			<tr>
				<th width="20%">排序参数:</th>
				<td>${busQueryFilter.sortparameter}</td>
			</tr>
			<tr>
				<th width="20%">是否共享:</th>
				<td>${busQueryFilter.isshare}</td>
			</tr>
			<tr>
				<th width="20%">创建时间:</th>
				<td>
				<fmt:formatDate value="${busQueryFilter.createtime}" pattern="yyyy-MM-dd"/>
				</td>
			</tr>
			<tr>
				<th width="20%">创建人ID:</th>
				<td>${busQueryFilter.createby}</td>
			</tr>
		</table>
		</div>
	</div>
</body>
</html>

