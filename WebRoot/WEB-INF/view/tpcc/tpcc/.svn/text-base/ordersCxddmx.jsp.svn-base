<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@include file="/commons/include/html_doctype.html" %>
<html>
<head>
<title>订单查询界面2</title>
<%@include file="/commons/include/get.jsp" %>
</head>
<body>
	<div class="panel">
		<form id="orders2" action="cxddmx.ht" method="post">
			<table class="table-detail" cellpadding="0" cellspacing="0" border="0">
				<tr>
					
					<th width="20%">订单号：</th>
					<td>
  <input type="text" name="ol_i_id" value=${orders2.ol_i_id} />
					</td>
				    <th width="20%">发货仓库号：</th>
					<td>
  <input type="text" name="ol_supply_w_id" value=${orders2.ol_supply_w_id} />
